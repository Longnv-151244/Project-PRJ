<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <filter>
        <filter-name>AdminLoginFitler</filter-name>
        <filter-class>filter.AdminLoginFitler</filter-class>
    </filter>
    <filter-mapping>
        <filter-name>AdminLoginFitler</filter-name>
        <url-pattern>/admin</url-pattern>
    </filter-mapping>
    <listener>
        <description>ServletContextListener</description>
        <listener-class>controller.MyListener</listener-class>
    </listener>
    <servlet>
        <servlet-name>SignInServlet</servlet-name>
        <servlet-class>controller.SignInServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SignOutServlet</servlet-name>
        <servlet-class>controller.SignOutServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SignUpServlet</servlet-name>
        <servlet-class>controller.SignUpServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>HomeServlet</servlet-name>
        <servlet-class>controller.HomeServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>VpopServlet</servlet-name>
        <servlet-class>controller.VpopServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>USUKServlet</servlet-name>
        <servlet-class>controller.USUKServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LofiServlet</servlet-name>
        <servlet-class>controller.LofiServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AdminServlet</servlet-name>
        <servlet-class>controller.AdminServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UpdateUserServlet</servlet-name>
        <servlet-class>controller.UpdateUserServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeleteCookieServlet</servlet-name>
        <servlet-class>controller.DeleteCookieServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CreateAlbumServlet</servlet-name>
        <servlet-class>controller.CreateAlbumServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UpdateAlbumServlet</servlet-name>
        <servlet-class>controller.UpdateAlbumServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeleteAlbumServlet</servlet-name>
        <servlet-class>controller.DeleteAlbumServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CreateLikedAlbumServlet</servlet-name>
        <servlet-class>controller.CreateLikedAlbumServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RemoveLikedAlbum</servlet-name>
        <servlet-class>controller.RemoveLikedAlbum</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UpAdminServlet</servlet-name>
        <servlet-class>controller.UpAdminServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>SignInServlet</servlet-name>
        <url-pattern>/sign-in</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SignOutServlet</servlet-name>
        <url-pattern>/sign-out</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SignUpServlet</servlet-name>
        <url-pattern>/sign-up</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>HomeServlet</servlet-name>
        <url-pattern>/home</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>VpopServlet</servlet-name>
        <url-pattern>/vpop</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>USUKServlet</servlet-name>
        <url-pattern>/us-uk</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LofiServlet</servlet-name>
        <url-pattern>/lofi</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AdminServlet</servlet-name>
        <url-pattern>/admin</url-pattern>
    </servlet-mapping>
    <welcome-file-list>
        <welcome-file>index.html</welcome-file>
    </welcome-file-list>
    <servlet-mapping>
        <servlet-name>UpdateUserServlet</servlet-name>
        <url-pattern>/user/update</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeleteCookieServlet</servlet-name>
        <url-pattern>/delete-cookie</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CreateAlbumServlet</servlet-name>
        <url-pattern>/admin/create/album</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UpdateAlbumServlet</servlet-name>
        <url-pattern>/admin/update/album</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeleteAlbumServlet</servlet-name>
        <url-pattern>/admin/delete/album</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CreateLikedAlbumServlet</servlet-name>
        <url-pattern>/create-liked</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RemoveLikedAlbum</servlet-name>
        <url-pattern>/remove-liked</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UpAdminServlet</servlet-name>
        <url-pattern>/admin/up-admin</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
